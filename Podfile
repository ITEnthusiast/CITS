source 'https://github.com/CocoaPods/Specs.git'
source 'https://git.coding.net/ITEnthusiast/CPSpecs.git'

platform :ios, '9.0'

target 'CITS' do

  use_frameworks!

  pod 'AFNetworking', '~> 3.1.0'

# 远程.公开.库
    pod 'CPDownLoaderPublic'

# 本地.私有.库
    pod 'CPDownLoaderLocal', :path => '../../Lib/CPLocalLib/CPDownLoaderLocal'
    
# 远程.私有.库
    pod 'DownLoaderPrivateCP'


end
